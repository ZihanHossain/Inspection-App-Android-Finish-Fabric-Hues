{
  "expo": {
    "name": "SQ Sticker",
    "slug": "sq_sticker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "sqhuses.sticker",
      "icon": "./assets/ic_icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0deceaf0-47dd-4ed7-943b-bd80e42c2521"
      }
    }
  }
}
